<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.eclipse.ice.viz.service.visit.preferencePage"
         name="VisIt Preferences"
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipse.ice.viz.service.preferences"
            class="org.eclipse.ice.viz.service.visit.connections.VisItConnectionPreferencePage"
            id="org.eclipse.ice.viz.service.visit.preferences"
            name="VisIt">
      </page>
   </extension>
   <extension
   		point="org.eclipse.core.runtime.preferences">
   		<initializer 
   			class="org.eclipse.ice.viz.service.visit.preferences.VisItPreferenceInitializer">
   		</initializer>
   </extension>
   <extension
         point="org.eclipse.ice.viz.service.IVizService">
      <client
            client="org.eclipse.ice.viz.service.visit.VisItVizService">
      </client>
   </extension>
</plugin>
