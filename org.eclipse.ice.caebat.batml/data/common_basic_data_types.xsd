<xsd:schema 
   xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
   xmlns="BatteryML:2.0"
   targetNamespace="BatteryML:2.0"
   elementFormDefault="qualified"
   attributeFormDefault="unqualified" version="2.0">

  <!--Simple Types-->

  <!--Single Value Types:
      Int32Type
      Int64Type
      NonNegativeIntType
      PositiveIntType
      NonNegativeInt32Type
      NonNegativeInt64Type
      Float32Type
      Float64Type
      StringType
      ShortStringType
      ShortTokenType
      ShortNameType
      BooleanType
      DateTimeType
      Encoded64Type
      EmbeddedXMLType
    -->
  <!--Single Value Elements
      Int32
      Int64
      NonNegativeInt
      PositiveInt
      NonNegativeInt32
      NonNegativeInt64
      Float32
      Float64
      String
      ShortString
      ShortToken
      ShortName
      Boolean
      DateTime
      Encoded64
      EmbeddedXML
    -->
  <!--List Value Types:
      Int32ListType
      Int64ListType
      PositiveIntListType
      NonNegativeIntListType
      NonNegativeInt32ListType
      NonNegativeInt64ListType
      Float32ListType
      Float64ListType
      ShortNameListType
      BooleanListType
      DateTimeListType
    -->
  <!--List Value Elements
      Int32List
      Int64List
      PositiveIntList
      NonNegativeIntList
      NonNegativeInt32List
      NonNegativeInt64List
      Float32List
      Float64List
      ShortNameList
      BooleanList
      DateTimeList
    -->

  <!--Complex Types-->

  <!--
      AnyValueType
      AnyNumericValueType
      AnyCounterValueType
      AnySingleValueType
      AnySingleNumericValueType
      AnySingleCounterValueType

      AnyListValueType
      AnyListNumericValueType
      AnyListCounterValueType
      AnySingleListValueType
      AnySingleListNumericValueType
      AnySingleListCounterValueType
    -->

  <xsd:element name="BasicDataTypesML_Doc">
    <xsd:annotation>
      <xsd:documentation>
	For testing.
      </xsd:documentation>
    </xsd:annotation>
    <xsd:complexType>
      <xsd:sequence>
	<xsd:any     namespace="##targetNamespace" minOccurs="0" maxOccurs="unbounded"/>
      </xsd:sequence>
    </xsd:complexType>
  </xsd:element>

  <!-- Simple Type Definitions-->

  <!--Simple Types, Single Values-->

  <xsd:element name="Int32" type="Int32Type">
    <xsd:annotation>
      <xsd:documentation>Individual integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Int32Type">
    <xsd:annotation>
      <xsd:documentation>Individual integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:int"/>
  </xsd:simpleType>

  <xsd:element name="Int64" type="Int64Type">
    <xsd:annotation>
      <xsd:documentation>Individual long integer value (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Int64Type">
    <xsd:annotation>
      <xsd:documentation>Individual long integer value (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:long"/>
  </xsd:simpleType>

  <xsd:element name="NonNegativeInt" type="NonNegativeIntType">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeIntType">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative Integer Value (32 bits, signed)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="NonNegativeInt32Type"/>
  </xsd:simpleType>

  <xsd:element name="PositiveInt" type="PositiveIntType">
    <xsd:annotation>
      <xsd:documentation>Individual positive integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="PositiveIntType">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative Integer Value (32 bits, signed)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="NonNegativeIntType">
      <xsd:minInclusive value="1"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:element name="NonNegativeInt32" type="NonNegativeInt32Type">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeInt32Type">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative Integer Value (32 bits, signed)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:unsignedInt"/>
  </xsd:simpleType>

  <xsd:element name="NonNegativeInt64" type="NonNegativeInt64Type">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative long integer value (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeInt64Type">
    <xsd:annotation>
      <xsd:documentation>Individual non-negative Integer Value (32 bits, signed)</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:unsignedLong"/>
  </xsd:simpleType>

  <xsd:element name="Float32" type="Float32Type">
    <xsd:annotation>
      <xsd:documentation>Individual 32-bit floating point value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Float32Type">
    <xsd:annotation>
      <xsd:documentation>Individual 32-bit floating point value.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:float"/>
  </xsd:simpleType>

  <xsd:element name="Float64" type="Float64Type">
    <xsd:annotation>
      <xsd:documentation>Individual 64-bit floating point value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Float64Type">
    <xsd:annotation>
      <xsd:documentation>Individual 64-bit floating point value.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:double"/>
  </xsd:simpleType>

  <xsd:element name="String" type="StringType">
    <xsd:annotation>
      <xsd:documentation>Individual string value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="StringType">
    <xsd:annotation>
      <xsd:documentation>Individual string value.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>

  <xsd:element name="ShortString" type="ShortStringType">
    <xsd:annotation>
      <xsd:documentation>Individual string value, limited to 1024 characters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ShortStringType">
    <xsd:annotation>
      <xsd:documentation>String with up to 1024 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string">
      <xsd:maxLength value="1024"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:element name="ShortToken" type="ShortTokenType">
    <xsd:annotation>
      <xsd:documentation>Individual token value, limited to 1024 characters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ShortTokenType">
    <xsd:annotation>
      <xsd:documentation>Token with up to 1024 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:token">
      <xsd:maxLength value="1024"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:element name="ShortName" type="ShortNameType">
    <xsd:annotation>
      <xsd:documentation>Individual name value, limited to 1024 characters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ShortNameType">
    <xsd:annotation>
      <xsd:documentation>Name with up to 1024 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:Name">
      <xsd:maxLength value="1024"/>
    </xsd:restriction>
  </xsd:simpleType>

  <xsd:element name="Boolean" type="BooleanType">
    <xsd:annotation>
      <xsd:documentation>Individual boolean value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="BooleanType">
    <xsd:annotation>
      <xsd:documentation>Individual boolean value.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:boolean"/>
  </xsd:simpleType>

  <xsd:element name="DateTime" type="DateTimeType">
    <xsd:annotation>
      <xsd:documentation>Individual ISO date/time value.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="DateTimeType">
    <xsd:annotation>
      <xsd:documentation>Individual ISO date/time value.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:dateTime"/>
  </xsd:simpleType>

  <xsd:element name="Encoded64" type="Encoded64Type">
    <xsd:annotation>
      <xsd:documentation>Base 64 encoded data.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Encoded64Type">
    <xsd:annotation>
      <xsd:documentation>Base 64 encoded PNG image.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:base64Binary"/>
  </xsd:simpleType>

  <xsd:element name="EmbeddedXML" type="EmbeddedXMLType">
    <xsd:annotation>
      <xsd:documentation>Value governed by a different XML Schema.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="EmbeddedXMLType">
    <xsd:annotation>
      <xsd:documentation>Value governed by a different XML Schema.</xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="xsd:string"/>
  </xsd:simpleType>

  <!--Simple Types, Lists-->

  <xsd:element name="Int32List" type="Int32ListType">
    <xsd:annotation>
      <xsd:documentation>List of integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Int32ListType">
    <xsd:annotation>
      <xsd:documentation>List of integer value (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="Int32Type"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="Int64List" type="Int64ListType">
    <xsd:annotation>
      <xsd:documentation>List of long integer value (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Int64ListType">
    <xsd:annotation>
      <xsd:documentation>List of long integer values (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="Int64Type"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="NonNegativeIntList" type="NonNegativeIntListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeIntListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="NonNegativeIntType"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="PositiveIntList" type="PositiveIntListType">
    <xsd:annotation>
      <xsd:documentation>List of positive positive integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="PositiveIntListType">
    <xsd:annotation>
      <xsd:documentation>List of positive integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="PositiveIntType"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="NonNegativeInt32List" type="NonNegativeInt32ListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeInt32ListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative integer values (32 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="NonNegativeInt32Type"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="NonNegativeInt64List" type="NonNegativeInt64ListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative long integer values (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="NonNegativeInt64ListType">
    <xsd:annotation>
      <xsd:documentation>List of non-negative long integer values (64 bits, signed).</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="NonNegativeInt64Type"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="Float32List" type="Float32ListType">
    <xsd:annotation>
      <xsd:documentation>List of 32-bit floating point values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Float32ListType">
    <xsd:annotation>
      <xsd:documentation>List of 32-bit floating point values.</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="xsd:float"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="Float64List" type="Float64ListType">
    <xsd:annotation>
      <xsd:documentation>List of 64-bit floating point values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="Float64ListType">
    <xsd:annotation>
      <xsd:documentation>List of 64-bit floating point values.</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="xsd:double"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="ShortNameList" type="ShortNameListType">
    <xsd:annotation>
      <xsd:documentation>List of short name values, limited to 1024 characters.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="ShortNameListType">
    <xsd:annotation>
      <xsd:documentation>Name with up to 1024 characters</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="xsd:Name">
	  <xsd:maxLength value="1024"/>
	</xsd:restriction>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="BooleanList" type="BooleanListType">
    <xsd:annotation>
      <xsd:documentation>List of boolean values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="BooleanListType">
    <xsd:annotation>
      <xsd:documentation>List of boolean values.</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="xsd:boolean"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <xsd:element name="DateTimeList" type="DateTimeListType">
    <xsd:annotation>
      <xsd:documentation>List of ISO date/time values.</xsd:documentation>
    </xsd:annotation>
  </xsd:element>
  <xsd:simpleType name="DateTimeListType">
    <xsd:annotation>
      <xsd:documentation>List of ISO date/time values.</xsd:documentation>
    </xsd:annotation>
    <xsd:list>
      <xsd:simpleType>
	<xsd:restriction base="xsd:dateTime"/>
      </xsd:simpleType>
    </xsd:list>
  </xsd:simpleType>

  <!-- Complex Type Definitions-->

  <!--Base Complex Type for Restriction-->
  <xsd:complexType name="AnyValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all series values. Can have multiple elements. To be restricted.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice maxOccurs="unbounded">
	<xsd:element ref="Int32"/>
	<xsd:element ref="Int64"/>
	<xsd:element ref="PositiveInt"/>
	<xsd:element ref="NonNegativeInt"/>
	<xsd:element ref="NonNegativeInt32"/>
	<xsd:element ref="NonNegativeInt64"/>
	<xsd:element ref="Float32"/>
	<xsd:element ref="Float64"/>
	<xsd:element ref="String"/>
	<xsd:element ref="ShortString"/>
	<xsd:element ref="ShortToken"/>
	<xsd:element ref="ShortName"/>
	<xsd:element ref="Boolean"/>
	<xsd:element ref="DateTime"/>
	<xsd:element ref="Encoded64"/>
	<xsd:element ref="EmbeddedXML"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AnyNumericValueType">
    <xsd:annotation>
      <xsd:documentation>Elements for numeric series values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="unbounded">
	    <xsd:element ref="Int32"/>
	    <xsd:element ref="Int64"/>
	    <xsd:element ref="PositiveInt"/>
	    <xsd:element ref="NonNegativeInt"/>
	    <xsd:element ref="NonNegativeInt32"/>
	    <xsd:element ref="NonNegativeInt64"/>
	    <xsd:element ref="Float32"/>
	    <xsd:element ref="Float64"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  
  <xsd:complexType name="AnyCounterValueType">
    <xsd:annotation>
      <xsd:documentation>Elements for counter series values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyNumericValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="unbounded">
	    <xsd:element ref="PositiveInt"/>
	    <xsd:element ref="NonNegativeInt"/>
	    <xsd:element ref="NonNegativeInt32"/>
	    <xsd:element ref="NonNegativeInt64"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>
  

<!--Unrestricted Complex Single Value-Based Types: Derived from Base Complex Value Type-->

  <xsd:complexType name="AnySingleValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all single values. To be restricted.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="1">
	    <xsd:element ref="Int32"/>
	    <xsd:element ref="Int64"/>
	    <xsd:element ref="PositiveInt"/>
	    <xsd:element ref="NonNegativeInt"/>
	    <xsd:element ref="NonNegativeInt32"/>
	    <xsd:element ref="NonNegativeInt64"/>
	    <xsd:element ref="Float32"/>
	    <xsd:element ref="Float64"/>
	    <xsd:element ref="String"/>
	    <xsd:element ref="ShortString"/>
	    <xsd:element ref="ShortToken"/>
	    <xsd:element ref="ShortName"/>
	    <xsd:element ref="Boolean"/>
	    <xsd:element ref="DateTime"/>
	    <xsd:element ref="Encoded64"/>
	    <xsd:element ref="EmbeddedXML"/>
	    <xsd:element ref="ChemicalElementSymbol"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AnySingleNumericValueType">
    <xsd:annotation>
      <xsd:documentation>Elements for numeric single values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyNumericValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="1">
	    <xsd:element ref="Int32"/>
	    <xsd:element ref="Int64"/>
	    <xsd:element ref="PositiveInt"/>
	    <xsd:element ref="NonNegativeInt"/>
	    <xsd:element ref="NonNegativeInt32"/>
	    <xsd:element ref="NonNegativeInt64"/>
	    <xsd:element ref="Float32"/>
	    <xsd:element ref="Float64"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AnySingleCounterValueType">
    <xsd:annotation>
      <xsd:documentation>Elements for single counter values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyCounterValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="1">
	    <xsd:element ref="PositiveInt"/>
	    <xsd:element ref="NonNegativeInt"/>
	    <xsd:element ref="NonNegativeInt32"/>
	    <xsd:element ref="NonNegativeInt64"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

<!-- Complex Types List Values-->

  <xsd:complexType name="AnyListValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all list series values. Can have multiple elements. To be restricted.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice maxOccurs="unbounded">
	<xsd:element ref="Int32List"/>
	<xsd:element ref="Int64List"/>
	<xsd:element ref="PositiveIntList"/>
	<xsd:element ref="NonNegativeIntList"/>
	<xsd:element ref="NonNegativeInt32List"/>
	<xsd:element ref="NonNegativeInt64List"/>
	<xsd:element ref="Float32List"/>
	<xsd:element ref="Float64List"/>
	<xsd:element ref="ShortNameList"/>
	<xsd:element ref="BooleanList"/>
	<xsd:element ref="DateTimeList"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AnyListNumericValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all numeric list values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyListValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="unbounded">
	    <xsd:element ref="Int32List"/>
	    <xsd:element ref="Int64List"/>
	    <xsd:element ref="PositiveIntList"/>
	    <xsd:element ref="NonNegativeIntList"/>
	    <xsd:element ref="NonNegativeInt32List"/>
	    <xsd:element ref="NonNegativeInt64List"/>
	    <xsd:element ref="Float32List"/>
	    <xsd:element ref="Float64List"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AnyListCounterValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all list counter values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnyListNumericValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="unbounded">
	    <xsd:element ref="PositiveIntList"/>
	    <xsd:element ref="NonNegativeIntList"/>
	    <xsd:element ref="NonNegativeInt32List"/>
	    <xsd:element ref="NonNegativeInt64List"/>
	  </xsd:choice>
	</xsd:sequence>
      </xsd:restriction>
    </xsd:complexContent>
  </xsd:complexType>

  <xsd:complexType name="AnySingleListValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all single list values. To be restricted.</xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:choice maxOccurs="1">
	<xsd:element ref="Int32List"/>
	<xsd:element ref="Int64List"/>
	<xsd:element ref="PositiveIntList"/>
	<xsd:element ref="NonNegativeIntList"/>
	<xsd:element ref="NonNegativeInt32List"/>
	<xsd:element ref="NonNegativeInt64List"/>
	<xsd:element ref="Float32List"/>
	<xsd:element ref="Float64List"/>
	<xsd:element ref="ShortNameList"/>
	<xsd:element ref="BooleanList"/>
	<xsd:element ref="DateTimeList"/>
      </xsd:choice>
    </xsd:sequence>
  </xsd:complexType>

  <xsd:complexType name="AnySingleListNumericValueType">
    <xsd:annotation>
      <xsd:documentation>Base type for all single numeric list values.</xsd:documentation>
    </xsd:annotation>
    <xsd:complexContent>
      <xsd:restriction base="AnySingleListValueType">
	<xsd:sequence>
	  <xsd:choice maxOccurs="1">
	    <xsd:element ref="Int32List"/>
	    <xsd:element ref="Int64List"/>
	    <xsd:element ref="PositiveIntList"/>
	    <xsd:element ref="NonNegativeIntList"/>
